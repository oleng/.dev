Host *
	UseKeychain		yes
	AddKeysToAgent		yes
	PubkeyAuthentication 	yes
	IdentitiesOnly 		yes
	HashKnownHosts 		yes
	ControlMaster 		auto
	IdentityFile 		~/.ssh/id_rsa
